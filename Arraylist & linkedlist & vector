package chocolate;
import java.util.*;

public class queuedemo {

	public static void main(String[] args) {
		Queue<Integer> queue = new LinkedList <Integer>();
		queue.add(01);
		queue.add(02);
		queue.add(03);
		queue.add(04);
		System.out.println("queue elements are - " +queue);
		System.out.println("queue front element - " +queue.element());
		

	}

}
