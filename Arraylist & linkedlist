package chocolate;
import java.util.*;
public class ArrayListDemo {

	public static void main(String[] args) {
		
		LinkedList<String> linkedlist=new LinkedList<String>();
		linkedlist.add("audi");
		linkedlist.add("bmw");
		linkedlist.add("benz");
		linkedlist.add("bentley");
		System.out.println(linkedlist);
		System.out.println("Arraylist after removing bmw from list");
		linkedlist.remove(1);
		System.out.println(linkedlist);

	}

}
